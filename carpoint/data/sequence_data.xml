<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <record id="rental_task_order" model="ir.sequence">
        <field name="name">Task Order</field>
        <field name="code">carpoint.rental.task</field>
        <field name="prefix">TA</field>
        <field name="padding">3</field>
        <field name="company_id" eval="False"/>
    </record>
    <record id="modification_task_order" model="ir.sequence">
        <field name="name">Task Order</field>
        <field name="code">carpoint.modification.task</field>
        <field name="prefix">MA</field>
        <field name="padding">3</field>
        <field name="company_id" eval="False"/>
    </record>
    <record id="repairing_task_order" model="ir.sequence">
        <field name="name">Task Order</field>
        <field name="code">carpoint.repairing.task</field>
        <field name="prefix">SA</field>
        <field name="padding">3</field>
        <field name="company_id" eval="False"/>
    </record>
</odoo>
